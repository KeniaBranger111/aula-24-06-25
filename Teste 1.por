programa {
  funcao inicio() {
  real numero1,numero2,soma,mult,div
  escreva("Escreva o primeiro numero : ")
  leia(numero1)
  escreva("Escreva o segundo numero : ")
  leia(numero2)
  soma=numero1+numero2
  mult=numero1*numero2
  div=numero1/numero2
  escreva("Soma: ",soma)
  escreva("Multi: ",mult)
  escreva("Div : ", div)
  }
}
